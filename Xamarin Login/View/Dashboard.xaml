<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             xmlns:forms="clr-namespace:Microcharts.Forms;assembly=Microcharts.Forms"
             x:Class="Xamarin_Login.View.Dashboard"
             Title="Dashboard">
    <ContentPage.Content>
        <StackLayout VerticalOptions="CenterAndExpand">
            <forms:ChartView x:Name="DeviceModelChart" HeightRequest="200" />
            <forms:ChartView x:Name="DeviceBrandChart" HeightRequest="200" />
            <forms:ChartView x:Name="DeviceOSChart" HeightRequest="200" />
        </StackLayout>
    </ContentPage.Content>
</ContentPage>